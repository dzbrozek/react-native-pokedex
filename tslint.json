{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
    "newline-before-return": false,
    "no-return-await": true,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-console": false,
    "no-empty": false,
    "ordered-imports": false,
    "member-access": false,
    "import-name": false,
    "max-line-length": [true, 120],
    "indent": [true, "spaces", 2]
  }
}